{
  "displayName": "Agent Data - Observability",
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "xPos": 0,
        "yPos": 0,
        "width": 6,
        "height": 6,
        "widget": {
          "title": "Ingest Success Rate (req/s)",
          "xyChart": {
            "chartOptions": {"mode": "COLOR"},
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "ingest_success_rps",
                "timeSeriesQuery": {
                  "prometheusQuery": "sum(rate(agent_ingest_success_total[5m]))"
                }
              }
            ]
          }
        }
      },
      {
        "xPos": 6,
        "yPos": 0,
        "width": 6,
        "height": 6,
        "widget": {
          "title": "Chat Messages Rate (msg/s)",
          "xyChart": {
            "chartOptions": {"mode": "COLOR"},
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "chat_msgs_rps",
                "timeSeriesQuery": {
                  "prometheusQuery": "sum(rate(agent_chat_messages_total[5m]))"
                }
              }
            ]
          }
        }
      },
      {
        "xPos": 0,
        "yPos": 6,
        "width": 12,
        "height": 8,
        "widget": {
          "title": "RAG Latency Quantiles (s)",
          "xyChart": {
            "chartOptions": {"mode": "COLOR"},
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "p50",
                "timeSeriesQuery": {
                  "prometheusQuery": "histogram_quantile(0.5, sum by (le) (rate(agent_rag_query_latency_seconds_bucket[5m])))"
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p95",
                "timeSeriesQuery": {
                  "prometheusQuery": "histogram_quantile(0.95, sum by (le) (rate(agent_rag_query_latency_seconds_bucket[5m])))"
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p99",
                "timeSeriesQuery": {
                  "prometheusQuery": "histogram_quantile(0.99, sum by (le) (rate(agent_rag_query_latency_seconds_bucket[5m])))"
                }
              }
            ]
          }
        }
      },
      {
        "xPos": 0,
        "yPos": 14,
        "width": 6,
        "height": 6,
        "widget": {
          "title": "API Requests (req/s)",
          "xyChart": {
            "chartOptions": {"mode": "COLOR"},
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "api_rps",
                "timeSeriesQuery": {
                  "prometheusQuery": "sum(rate(starlette_requests_total[5m]))"
                }
              }
            ]
          }
        }
      },
      {
        "xPos": 6,
        "yPos": 14,
        "width": 6,
        "height": 6,
        "widget": {
          "title": "API Latency Quantiles (s)",
          "xyChart": {
            "chartOptions": {"mode": "COLOR"},
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "p50",
                "timeSeriesQuery": {
                  "prometheusQuery": "histogram_quantile(0.5, sum by (le) (rate(starlette_requests_processing_time_seconds_bucket[5m])))"
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p95",
                "timeSeriesQuery": {
                  "prometheusQuery": "histogram_quantile(0.95, sum by (le) (rate(starlette_requests_processing_time_seconds_bucket[5m])))"
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p99",
                "timeSeriesQuery": {
                  "prometheusQuery": "histogram_quantile(0.99, sum by (le) (rate(starlette_requests_processing_time_seconds_bucket[5m])))"
                }
              }
            ]
          }
        }
      }
    ]
  }
}

